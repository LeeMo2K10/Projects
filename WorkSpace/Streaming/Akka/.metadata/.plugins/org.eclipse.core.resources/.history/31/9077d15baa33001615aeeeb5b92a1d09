package com.akka.distribute;

import akka.persistence.UntypedPersistentActor;

public class Counter extends UntypedPersistentActor {

	@Override
	public String persistenceId() {
		return "Counter-" + getSelf().path().name();
	}

	@Override
	public void onReceiveCommand(Object arg0) throws Exception {

	}

	@Override
	public void onReceiveRecover(Object arg0) throws Exception {

	}

}

<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified"
	elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:element name="Company">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Employee" minOccurs="1" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element type="xs:integer" name="EmpID" maxOccurs="1"
								minOccurs="1"></xs:element>
							<xs:element type="xs:string" name="FirstName"
								maxOccurs="1" minOccurs="1"></xs:element>
							<xs:element type="xs:string" name="LastName"
								maxOccurs="1" minOccurs="1"></xs:element>
							<xs:element type="xs:long" name="MobileNo" maxOccurs="1"
								minOccurs="1"></xs:element>
						</xs:sequence>
						<xs:attribute name="EmpID" type="xs:string" use="required"></xs:attribute>
					</xs:complexType>
				</xs:element>

				<xs:element name="Emp_parent">
					<xs:complexType>
						<xs:sequence>
							<xs:element type="xs:string" name="Emp_Father"
								maxOccurs="1" minOccurs="1"></xs:element>
							<xs:element type="xs:string" name="Emp_Mother"
								maxOccurs="1" minOccurs="1"></xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

</xs:schema>